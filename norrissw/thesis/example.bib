	keywords = {Bacteria, Computational biology and bioinformatics, Genetic techniques, Metabolomics},
	pages = {977--982},
	file = {Snapshot:/home/snorris/.zotero/zotero/o03p7748.default/zotero/storage/MH5TRRBV/nbt.1672.html:text/html}
}

@article{li_identification_2009,
	title = {Identification of {Potential} {Pathway} {Mediation} {Targets} in {Toll}-like {Receptor} {Signaling}},

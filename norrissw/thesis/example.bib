@article{thiele_genome-scale_2009,
	title = {Genome-{Scale} {Reconstruction} of {Escherichia} coli's {Transcriptional} and {Translational} {Machinery}: {A} {Knowledge} {Base}, {Its} {Mathematical} {Formulation}, and {Its} {Functional} {Characterization}},
